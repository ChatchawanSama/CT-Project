<AnchorPane fx:id="container" prefHeight="768.0" prefWidth="1024.0" styleClass="secondary-color" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.UserProfileController">
