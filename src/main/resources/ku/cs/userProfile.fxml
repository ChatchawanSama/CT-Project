                        <ImageView fitHeight="35.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@images/CtRemoveBg.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Label alignment="CENTER" contentDisplay="CENTER" layoutX="100.0" layoutY="6.0" prefHeight="51.0" prefWidth="169.0" text="My Profile" textFill="#878787">
